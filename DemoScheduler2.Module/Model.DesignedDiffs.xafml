<?xml version="1.0" ?>
<Application Title="DemoScheduler2">
  <Options>
    <LayoutManagerOptions RequiredFieldMark="*" />
  </Options>
  <Views>
    <DetailView Id="Dashboard_DetailView">
      <Layout>
        <LayoutGroup Id="Main" RelativeSize="100">
          <LayoutGroup Id="SimpleEditors" RelativeSize="30">
            <LayoutItem Id="Name" RelativeSize="50" />
            <LayoutItem Id="Description" RelativeSize="50" />
          </LayoutGroup>
          <TabbedGroup Id="TabContainer" RelativeSize="70">
            <LayoutGroup Id="GridEvents" Text="Grid Events" RelativeSize="100">
              <LayoutItem Id="GridEvents" RelativeSize="100" />
            </LayoutGroup>
            <LayoutGroup Id="SchedulerEvents" Text="Scheduler Events" RelativeSize="100">
              <LayoutItem Id="SchedulerEvents" RelativeSize="100" />
            </LayoutGroup>
          </TabbedGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <ListView Id="Dashboard_GridEvents_ListView" EditorTypeName="DevExpress.ExpressApp.Editors.GridListEditor" />
    <ListView Id="Dashboard_SchedulerEvents_ListView" EditorTypeName="DevExpress.ExpressApp.Scheduler.SchedulerListEditor" />
  </Views>
</Application>
